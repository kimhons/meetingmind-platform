{
  "name": "meetingmind-platform",
  "version": "1.0.0",
  "description": "AI-Powered Meeting Assistant with AIMLAPI Integration - Revolutionary 70% cost savings with superior intelligence",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "test:integration": "node test-aimlapi-integration-fixed.js",
    "test:aimlapi": "node validate-aimlapi.js",
    "test:all": "npm run test:integration && npm run test:aimlapi",
    "install:windows": "install/windows/install-meetingmind.bat",
    "install:macos": "bash install/macos/install-meetingmind.sh",
    "install:unix": "sudo bash install/unix/install-meetingmind.sh",
    "docker:build": "docker build -t meetingmind:latest .",
    "docker:run": "docker run -p 3000:3000 --env-file .env meetingmind:latest",
    "docker:compose": "docker-compose up -d",
    "docker:stop": "docker-compose down",
    "health": "curl -f http://localhost:3000/health || exit 1",
    "deploy:railway": "railway deploy",
    "setup": "node scripts/setup.js"
  },
  "keywords": [
    "ai",
    "meeting",
    "assistant",
    "transcription",
    "analysis",
    "aimlapi",
    "zoom",
    "teams",
    "google-meet",
    "interview",
    "intelligence",
    "automation"
  ],
  "author": {
    "name": "MeetingMind Team",
    "email": "support@meetingmind.com",
    "url": "https://github.com/kimhons/meetingmind-platform"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kimhons/meetingmind-platform.git"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.65.0",
    "axios": "^1.12.2",
    "cheerio": "^1.1.2",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "framer-motion": "^12.23.22",
    "googleapis": "^161.0.0",
    "lucide-react": "^0.545.0",
    "node-fetch": "^2.7.0",
    "openai": "^6.2.0",
    "ws": "^8.18.3"
  }
}

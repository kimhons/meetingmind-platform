# Hybrid AI Provider Configuration for MeetingMind
# Primary: AIMLAPI for cost efficiency
# Backup: Direct providers for reliability

# AIMLAPI Configuration (Primary Provider)
AIMLAPI_API_KEY=your_aimlapi_key_here
AIMLAPI_BASE_URL=https://api.aimlapi.com/v1

# OpenAI Configuration (Backup Provider)
OPENAI_API_KEY=your_openai_key_here
OPENAI_BASE_URL=https://api.openai.com/v1

# Anthropic Configuration (Backup Provider)
ANTHROPIC_API_KEY=your_anthropic_key_here
ANTHROPIC_BASE_URL=https://api.anthropic.com/v1

# Google AI Configuration (Backup Provider)
GOOGLE_API_KEY=your_google_key_here
GOOGLE_BASE_URL=https://generativelanguage.googleapis.com/v1beta

# AI Provider Strategy Configuration
AI_STRATEGY=hybrid
PRIMARY_PROVIDER=aimlapi
FALLBACK_PROVIDERS=openai,anthropic,google

# Cost and Performance Targets
TARGET_COST_SAVINGS=70
MAX_LATENCY_MS=2000
MIN_SUCCESS_RATE=95

# Health Monitoring Configuration
HEALTH_CHECK_INTERVAL=60000
PROVIDER_TIMEOUT=30000
MAX_RETRIES=3

# Usage Analytics
ENABLE_USAGE_TRACKING=true
COST_TRACKING=true
PERFORMANCE_MONITORING=true

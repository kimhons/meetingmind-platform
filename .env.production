# MeetingMind Production Environment Configuration
# AIMLAPI Integration - Week 1 Implementation

# ================================
# AIMLAPI PRIMARY PROVIDER CONFIG
# ================================
AIMLAPI_API_KEY=your_aimlapi_key_here
AIMLAPI_BASE_URL=https://api.aimlapi.com/v1
AIMLAPI_TIMEOUT=30000
AIMLAPI_RETRIES=3
AIMLAPI_RATE_LIMIT_ENABLED=true

# AIMLAPI Model Configuration
AIMLAPI_DEFAULT_MODEL=gpt-5-pro
AIMLAPI_REALTIME_MODEL=grok-4-fast
AIMLAPI_MONITORING_MODEL=deepseek-v3.1
AIMLAPI_MULTILINGUAL_MODEL=qwen3-max

# ================================
# FALLBACK PROVIDERS CONFIG
# ================================

# OpenAI Fallback (Tier 1)
OPENAI_API_KEY=your_openai_key_here
OPENAI_BASE_URL=https://api.openai.com/v1
OPENAI_DEFAULT_MODEL=gpt-4o
OPENAI_TIMEOUT=30000
OPENAI_RETRIES=2

# Google Fallback (Tier 2)
GOOGLE_API_KEY=your_google_key_here
GOOGLE_BASE_URL=https://generativelanguage.googleapis.com/v1beta
GOOGLE_DEFAULT_MODEL=gemini-pro
GOOGLE_TIMEOUT=30000
GOOGLE_RETRIES=2

# Anthropic Fallback (Tier 3)
ANTHROPIC_API_KEY=your_anthropic_key_here
ANTHROPIC_BASE_URL=https://api.anthropic.com/v1
ANTHROPIC_DEFAULT_MODEL=claude-3-sonnet-20240229
ANTHROPIC_TIMEOUT=30000
ANTHROPIC_RETRIES=2

# ================================
# COST MANAGEMENT CONFIG
# ================================
AI_MONTHLY_BUDGET=5000
AI_COST_ALERT_THRESHOLD_70=3500
AI_COST_ALERT_THRESHOLD_85=4250
AI_COST_ALERT_THRESHOLD_95=4750
AI_COST_TRACKING_ENABLED=true
AI_BUDGET_ENFORCEMENT_ENABLED=true

# Cost per 1K tokens (in USD)
COST_GPT5_PRO=0.00656
COST_CLAUDE_45_SONNET=0.01890
COST_GROK_4_FAST=0.000735
COST_DEEPSEEK_V31=0.002352
COST_QWEN3_MAX=0.008400
COST_GEMINI_25_FLASH=0.003150

# ================================
# PERFORMANCE CONFIG
# ================================
AI_RESPONSE_TIMEOUT_REALTIME=500
AI_RESPONSE_TIMEOUT_STANDARD=2000
AI_RESPONSE_TIMEOUT_BATCH=5000
AI_QUALITY_SCORE_THRESHOLD=0.8
AI_CACHE_ENABLED=true
AI_CACHE_TTL=3600

# ================================
# RATE LIMITING CONFIG
# ================================
RATE_LIMIT_GPT5_PRO=100
RATE_LIMIT_CLAUDE_45_SONNET=50
RATE_LIMIT_GROK_4_FAST=200
RATE_LIMIT_DEEPSEEK_V31=500
RATE_LIMIT_WINDOW=60000

# ================================
# HEALTH MONITORING CONFIG
# ================================
HEALTH_CHECK_INTERVAL=30000
HEALTH_FAILURE_THRESHOLD=3
HEALTH_RECOVERY_THRESHOLD=2
PROVIDER_CIRCUIT_BREAKER_ENABLED=true

# ================================
# LOGGING AND MONITORING
# ================================
AI_LOGGING_LEVEL=info
AI_METRICS_ENABLED=true
AI_PERFORMANCE_TRACKING=true
AI_COST_ANALYTICS=true

# ================================
# SECURITY CONFIG
# ================================
AI_API_KEY_ROTATION_ENABLED=true
AI_API_KEY_ROTATION_INTERVAL=2592000
AI_REQUEST_ENCRYPTION=true
AI_RESPONSE_SANITIZATION=true

# ================================
# FEATURE FLAGS
# ================================
FEATURE_MULTI_MODEL_SYNTHESIS=true
FEATURE_CONTEXT_AWARE_ROUTING=true
FEATURE_PREDICTIVE_INTELLIGENCE=true
FEATURE_INDUSTRY_SPECIALIZATION=true
FEATURE_REAL_TIME_LEARNING=true

# ================================
# SUPABASE CONFIG
# ================================
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# ================================
# VERCEL CONFIG
# ================================
VERCEL_TOKEN=your_vercel_token
VERCEL_PROJECT_ID=your_vercel_project_id
VERCEL_ORG_ID=your_vercel_org_id

# ================================
# MEETING PLATFORM INTEGRATIONS
# ================================
MEETING_BAAS_API_KEY=your_meeting_baas_key
ZOOM_CLIENT_ID=your_zoom_client_id
ZOOM_CLIENT_SECRET=your_zoom_client_secret
TEAMS_CLIENT_ID=your_teams_client_id
TEAMS_CLIENT_SECRET=your_teams_client_secret
GOOGLE_MEET_CLIENT_ID=your_google_meet_client_id
GOOGLE_MEET_CLIENT_SECRET=your_google_meet_client_secret

# ================================
# DEVELOPMENT CONFIG
# ================================
NODE_ENV=production
PORT=3000
DATABASE_URL=postgresql://user:password@localhost:5432/meetingmind

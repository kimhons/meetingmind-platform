# Meeting BaaS Integration Configuration
# Enhanced Platform Integrations for Seamless Meeting Join

# Meeting BaaS API Configuration
MEETING_BAAS_API_KEY=your_meeting_baas_api_key_here
MEETING_BAAS_BASE_URL=https://api.meetingbaas.com
WEBHOOK_BASE_URL=https://api.meetingmind.com/webhooks

# Platform-Specific API Keys
ZOOM_CLIENT_ID=your_zoom_client_id
ZOOM_CLIENT_SECRET=your_zoom_client_secret
ZOOM_WEBHOOK_SECRET=your_zoom_webhook_secret

TEAMS_TENANT_ID=your_azure_tenant_id
TEAMS_CLIENT_ID=your_teams_client_id
TEAMS_CLIENT_SECRET=your_teams_client_secret

GOOGLE_CLIENT_ID=your_google_client_id
GOOGLE_CLIENT_SECRET=your_google_client_secret
GOOGLE_CALENDAR_API_KEY=your_google_calendar_api_key

WEBEX_CLIENT_ID=your_webex_client_id
WEBEX_CLIENT_SECRET=your_webex_client_secret
WEBEX_PERSONAL_ACCESS_TOKEN=your_webex_personal_token

# Amazon Chime SDK Configuration
AWS_ACCESS_KEY_ID=your_aws_access_key
AWS_SECRET_ACCESS_KEY=your_aws_secret_key
AWS_REGION=us-east-1
CHIME_APP_INSTANCE_ARN=your_chime_app_instance_arn

# Gong Integration
GONG_CLIENT_ID=your_gong_client_id
GONG_CLIENT_SECRET=your_gong_client_secret
GONG_ACCESS_TOKEN=your_gong_access_token

# Calendar Integration Settings
GOOGLE_CALENDAR_ENABLED=true
OUTLOOK_CALENDAR_ENABLED=true
CALENDAR_AUTO_JOIN_ENABLED=true
CALENDAR_CHECK_INTERVAL=30000

# Bot Configuration
DEFAULT_BOT_NAME=MeetingMind AI Assistant
DEFAULT_RECORDING_MODE=speaker_view
DEFAULT_ENTRY_MESSAGE=MeetingMind AI Assistant has joined to provide intelligent insights
BOT_AVATAR_URL=https://meetingmind.com/assets/bot-avatar.png

# Real-Time Processing
WEBSOCKET_ENABLED=true
REAL_TIME_TRANSCRIPTION=true
LIVE_INSIGHTS_ENABLED=true
PARTICIPANT_DETECTION=true

# Webhook Configuration
WEBHOOK_SECRET=your_webhook_secret_key
WEBHOOK_TIMEOUT=30000
WEBHOOK_RETRY_ATTEMPTS=3

# Performance Settings
MAX_CONCURRENT_MEETINGS=10
CONNECTION_TIMEOUT=30000
RETRY_ATTEMPTS=3
RATE_LIMIT_REQUESTS_PER_MINUTE=60

# Security Settings
ENCRYPTION_ENABLED=true
DATA_RETENTION_DAYS=30
GDPR_COMPLIANCE=true
HIPAA_COMPLIANCE=true

# Feature Flags
SEAMLESS_JOIN_ENABLED=true
CALENDAR_AUTO_JOIN=true
REAL_TIME_PROCESSING=true
MULTI_PLATFORM_SUPPORT=true
FALLBACK_OPTIONS=true

# Logging and Monitoring
LOG_LEVEL=info
PERFORMANCE_MONITORING=true
ERROR_TRACKING=true
ANALYTICS_ENABLED=true
